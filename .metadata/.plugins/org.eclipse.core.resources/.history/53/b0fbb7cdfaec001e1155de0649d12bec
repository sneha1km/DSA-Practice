
public class FindMinimumNumber {
	static int findMinNumber(int [] array) {
		if(array.length==0) {
			return -99999999;
		}
		int min=array[0];
		for(int i=1;i<array.length;i++) {
			if(array[i]<min) {
				min=array[i];
			}
		}
		return min;
	}

	public static void main(String[] args) {
	  int arr[]= {};
	  
	  System.out.println(findMinNumber(arr));

	}

}
